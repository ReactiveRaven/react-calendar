{
  "name": "rr-calendar",
  "keywords": [
    "react",
    "calendar",
    "material",
    "materialui",
    "material-ui"
  ],
  "version": "1.0.1",
  "engines": {
    "node": ">=10 <15"
  },
  "private": false,
  "license": "MIT",
  "main": "lib/entrypoint.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/reactiveraven/rr-calendar.git"
  },
  "types": "lib/entrypoint.d.ts",
  "dependencies": {
    "autoprefixer": "9.8.6",
    "babel-jest": "20.0.3",
    "babel-loader": "8.2.2",
    "babel-preset-react-app": "^3.1.2",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "chalk": "4.1.0",
    "css-loader": "0.28.11",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "9.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "26.6.3",
    "keyboardevent-key-polyfill": "^1.1.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "5.0.1",
    "postcss-loader": "3.0.0",
    "promise": "8.1.0",
    "raf": "3.4.1",
    "react-dev-utils": "^5.0.2",
    "resolve": "1.19.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.23.1",
    "sw-precache-webpack-plugin": "1.0.0",
    "timezone-support": "^2.0.2",
    "ts-jest": "26.4.4",
    "ts-loader": "^8.0.11",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^4.1.0",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "1.1.2",
    "webpack": "3.12.0",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "2.2.0",
    "whatwg-fetch": "3.5.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "npm run clean && npm run test:once && npm run build:lib && npm run build:umd",
    "build:lib": "./node_modules/.bin/tsc --outDir lib --module commonjs",
    "build:es": "./node_modules/.bin/tsc --outDir es --module es2015",
    "build:umd": "npm run build:es && dts-bundle --name dist/bundle --main es --outputAsModuleFolder",
    "clean": "rimraf dist es lib coverage",
    "test": "node scripts/test.js --env=jsdom",
    "test:once": "CI=true node scripts/test.js --env=jsdom",
    "postbuild": "rimraf {lib,es}/**/__tests__ {lib,es}/**/*.{spec,test}.{js,d.ts,js.map}",
    "ci": "npm run build && node scripts/dev-test.js"
  },
  "peerDependencies": {
    "@material-ui/core": ">=1.4.3 <4.0.0",
    "@material-ui/icons": ">=2.0.3 <4.0.0",
    "enzyme-adapter-react-16": "1.x",
    "react": ">=16.4.1 <17.0.0",
    "react-dom": ">=16.4.1 <17.0.0"
  },
  "devDependencies": {
    "@material-ui/core": "^3.9.4",
    "@material-ui/icons": "^3.0.2",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^17.0.0",
    "@types/sinon": "^9.0.9",
    "cz-conventional-changelog": "^3.3.0",
    "dts-bundle": "^0.7.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "pascal-case": "^3.1.2",
    "react": "^16.14.0",
    "react-dom": ">=16.4.1 <17.0.0",
    "semantic-release": "^17.3.0",
    "sinon": "^9.2.1",
    "typescript": "^4.1.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.ts",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
